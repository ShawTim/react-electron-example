{
  "name": "react-electron-example",
  "version": "0.1.0",
  "private": true,
  "main": "electron.js",
  "description": "Simple Secure Contact Manager",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "7.12.3",
    "@electron-forge/cli": "^6.0.0-beta.59",
    "@pmmmwh/react-refresh-webpack-plugin": "0.4.3",
    "@svgr/webpack": "5.5.0",
